<!doctype html>
<html lang=en>
{% block head %}
<title>{% block title %}{% endblock %}</title>
{% block css %}
<link rel="stylesheet" href="/static/css/w3.css">
<link rel="stylesheet" href="/static/css/ckeditor-custom.css">
<link rel="stylesheet" href="/static/css/fonts.css">
<link rel="stylesheet" href="/static/css/variables.css">
<link rel="stylesheet" href="/static/css/global.css">

<link rel="stylesheet" href="/static/css/menu.css">
<link rel="stylesheet" href="/static/css/switch.css">
<!--To be able to access ARIA-->
<script src="/static/js/table.js"></script>
<script src="/static/js/menu.js"></script>

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="/static/js/ckeditor5-36/build/ckeditor.js"></script>


{% endblock css %}
{% block extend_head %}{% endblock extend_head %}
{% endblock head %}
<body>
{% include "header.html.tera" %}
<main>
  {% if flash %}
  <div id="alertDiv" class="heading-borderless w3-padding">
    <div role="alert">
      {{ flash.kind }}: {{ flash.message }}
    </div>
  </div>
  {% endif %}
  <div class="w3-main" id="main">
    {% block content %}{% endblock %}
  </div>
</main>
<!-- END GRID -->
</div>
<div id="arrow-keys-indicator" class="hidden"></div>
<!-- END w3-content -->
</div>
{% block scripts %}
<script>
  setTimeout(() => {
    let get = document.getElementById('alertDiv');
    get.style.display = 'none';

  }, 5000);
</script>

{% endblock scripts %}
</body>
</html>